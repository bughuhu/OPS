WHERE  OBJECTID1 = ? AND OBJECTVALUE1 = ? AND  OBJECTID2 = ? AND 
OBJECTVALUE2 = ? AND  OBJECTID3 = ? AND OBJECTVALUE3 = ? AND  OBJECTID4 = ? 
AND OBJECTVALUE4 = ? AND  OBJECTID5 = ? AND OBJECTVALUE5 = ? AND  
OBJECTID6 = ? AND OBJECTVALUE6 = ? AND  OBJECTID7 = ? AND 
OBJECTVALUE7 = ? ORDER BY NAMENUM

