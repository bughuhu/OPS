<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
    	<Console name="Console-Less" target="SYSTEM_OUT">
    		<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level - %msg%n"/>
    	</Console>
    	<Console name="Console-More" target="SYSTEM_OUT">
    		<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    	</Console>
    </Appenders>
    <Loggers>
    	<Root level="all">
    		<AppenderRef ref="Console-Less"/>
    	</Root>
		<Logger name="com.enterrupt.buffers.RecordPCListRequestBuffer" level="WARN"/>
    </Loggers>
</Configuration>
